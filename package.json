{
  "name": "branch-check-cli",
  "version": "0.0.2",
  "description": "Validate your branch naming strategy.",
  "main": "public/index.js",
  "scripts": {
    "compile": "tsc",
    "prepublishOnly": "npm run compile",
    "test:valid": "ts-node src/index.ts -d 'feature/ bug/ master' && ts-node src/index.ts -m '*'",
    "invalid": "ts-node src/index.ts -d 'foobar/' && ts-node src/index.ts -m '*.*'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:valid"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssulivan11/branch-check-cli.git"
  },
  "author": "Scott Sullivan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ssulivan11/branch-check-cli/issues"
  },
  "homepage": "https://github.com/ssulivan11/branch-check-cli#readme",
  "devDependencies": {
    "@types/node": "^14.0.27",
    "chalk": "^4.1.0",
    "commander": "^6.0.0",
    "findup-sync": "^4.0.0",
    "git-branch": "^2.0.1",
    "husky": "^4.2.5",
    "shelljs": "^0.8.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "wildcard-match": "^4.0.0"
  }
}
